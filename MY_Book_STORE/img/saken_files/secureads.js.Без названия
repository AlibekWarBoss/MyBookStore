IRmyfunction();
if(stopadsclick>0){
var time=parseInt(stopadsclick)*1000;
IrStopAdsClicking();
setTimeout(function(){ Irreinit(); Irinit();  }, time);
}else{  
Irreinit();
Irinit();
}
}
