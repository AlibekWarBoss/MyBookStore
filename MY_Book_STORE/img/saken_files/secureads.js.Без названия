document.addEventListener("visibilitychange", function() { window.focus();
if (document.hidden) {isTabActive = false;}
else {var incRevBlockAll = readCookie("IRcookieTime");
if(incRevBlockAll){var obj = JSON.parse(incRevBlockAll);
