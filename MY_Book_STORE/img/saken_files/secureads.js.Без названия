}
var adConten='<div class="InMargin_Top" '+unitcss+'><div '+btncss+' onclick="this.parentNode.style.display = \'none\';"> '+foterclosebtn+' </div><article><ins '+AdClass+' '+adscss+' '+pubID+' '+adSlot+' ></ins></article></div>';
var AdsScript = document.createElement('script');
AdsScript.setAttribute('src', scriptSRC);
var AdsScript2 = document.createElement('script');
AdsScript2.setAttribute('src', scriptSRC2);
x[i].innerHTML=adConten;
x[i].appendChild(AdsScript);
x[i].appendChild(AdsScript2);
x[i].appendChild(googleadsPush());
x[i].setAttribute("AdsStatus","Done");
x[i].setAttribute("Ir-Ads-Unit","s970x902");
}}}
