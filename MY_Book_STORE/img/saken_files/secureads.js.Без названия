var adscss=''; var adkey=x[i].getAttribute('data-ad-layout-key'); var adslot_=x[i].getAttribute('data-ad-slot');
var AdsEvent_='';
if(parseInt(stopadsclick)>0){
AdsEvent_=IRpointerEvents();
}
var adSlot='data-ad-slot="'+adslot_+'"';
adscss='style="display:block;'+AdsEvent_+'"';
var adConten='<article><ins '+AdClass+' '+adscss+' style="'+AdsEvent_+'" data-ad-format="fluid" data-ad-layout-key="'+adkey+'" '+pubID+' '+adSlot+'></ins></article>';
